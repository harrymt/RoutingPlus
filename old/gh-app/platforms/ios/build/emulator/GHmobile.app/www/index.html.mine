
<!DOCTYPE html>
<html>
    <head>
        <!-- Standard Meta tags -->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta name="keywords" content="road routing,shortest path,maps,openstreetmap,android,navigation,routenplaner,gis"/>
        <!-- Description of the website in google -->
        <meta name="description" content="Get driving directions and view maps in GraphHopper Maps">
        <!-- Icon picture -->
        <link type="image/png" rel="icon" href="/favicon.ico"/>

        <title>Driving Directions - GraphHopper Maps</title>

        <!-- imported leaflet css style -->
        <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.7.2/leaflet.css" />
        <!-- TODO download this file  -->
        <!--[if lte IE 8]>
            <link rel="stylesheet" href="css/leaflet.ie.css" />
        <![endif]-->

        <!-- Custom sidebar leaflet -->
    <link rel="stylesheet" href="css/L.Control.Sidebar.css" />

        <!-- Custom style -->
        <link rel="stylesheet" type="text/css" href="css/style.css" />

        <!-- Custom input style -->
        <link rel="stylesheet" type="text/css" href="css/input.css" />


        <!-- MAPBOX files -->
        <script src='https://api.tiles.mapbox.com/mapbox.js/v1.6.1/mapbox.js'></script>

        <link href='https://api.tiles.mapbox.com/mapbox.js/v1.6.1/mapbox.css' rel='stylesheet' />

        <!-- Mapui css -->
        <link rel="stylesheet" type="text/css" href="css/mapui.css" />

        <!-- Font aweseome -->
        <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">

        <!-- The given style -->
        <link rel="stylesheet" type="text/css" href="css/mystyle.css" />

        <!-- Jquery, autocomplete, history -->
        <script type="text/javascript" src="js/jquery-1.10.2.min.js"></script>
        <script type="text/javascript" src="js/jquery.history.js"></script>

        <!-- <script type="text/javascript" src="js/jquery.autocomplete.js"></script> -->

        <!-- Leaflet and sidebar js -->
        <script src="http://cdn.leafletjs.com/leaflet-0.7.2/leaflet.js"></script>

        <script src="js/L.Control.Sidebar.js"></script>

        <!-- Graph hopper request -->
        <script type="text/javascript" src="js/ghrequest.js?v=3"></script>

        <!-- Note this must be below ghrequest -->
        <script type="text/javascript" src="js/main.js?v=14"></script>
        <script type="text/javascript" src="phonegap.js"></script>
        <script type="text/javascript" src="js/index.js"></script>
        <script type="text/javascript">
            app.initialize();
        </script>
    </head>
    <body>
    <div id="sidebar">
        <!-- The sidebar / navbar -->
        <div id="input">
                <div class="toggle-topleft input-toggle">
                <ul>
                <li><a href='#' class='active' id='toggle-sidebar-input'>
                  <i class="fa fa-bars fa-3"></i> Map</a>
                  <a href="preferences.html"><img src="img/car.png" alt="Preference" /></a></li>
                </ul>

                </div>

                <!-- Buttons -->
                <div id="options">
                    <span id="vehicles">
                    </span>
                </div>

                <!-- Logo -->
                <div id="header_img">
                    <a class="no_link" href="http://graphhopper.com">
                    <img alt="GraphHopper" src="http://graphhopper.com/img/header.png"/>
                    </a>
                </div>
                <div class="toggle_btn">
                </div>



                <form id="locationform">
                    <!-- From and To text boxes -->
                    <div id="locationpoints">

                        <div id="fromDiv">
                            <img id="fromIndicator" class="hidden" src="img/indicator.gif"/>
                            <img id="fromFlag" src="img/marker-small-green.png"/>
                            <input id="fromInput" type="text" placeholder="From"/>
                            <div class="clear"> </div>
                            <div id="fromResolveFound"></div>
                            <div id="fromResolveError"></div>
                        </div>

                        <div class="clear"> </div>

                        <div id="toDiv">
                            <img id="toIndicator" class="hidden" src="img/indicator.gif"/>
                            <img id="toFlag" src="img/marker-small-red.png"/>
                            <input id="toInput" type="text" placeholder="To"/>
                            <div class="clear"> </div>
                            <div id="toResolveFound"></div>
                            <div id="toResolveError"></div>
                        </div>
                    </div>

                    <div class="clear"> </div>
                    <!-- Link map icon -->
                    <div id="exportLink" title="Static Link" class="left"><a href="/maps"><img src='img/link.png'></a></div>
                    <!-- Search button -->
                    <input id="searchButton" type="submit" value="Search">
                </form>

                <!-- gpx map -->
                <div id="gpxExportButton" title="GPX Download">
                    <a href=""><img alt="gpx" src='img/gpx.png'></a>
                </div>


            <div class="clear"> </div>

            <div id="info" class="small_text"></div>
            <div id="error" class="error"></div>
        </div>
        </div>

        <!-- Map -->
        <div id="map">
        </div>
        <!-- Elements on map -->
        <div id='map-ui' class="toggle-topleft map-toggle">
        <ul>
          <li><a href='#' class='active' id='toggle-sidebar'>
          <i class="fa fa-bars fa-3"></i> Search</a></li>
        </ul>
        </div>

    </body>
</html>
